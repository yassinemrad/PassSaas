@model IEnumerable<WebMap.Models.reclamationViewModel>

@{
    ViewBag.Title = "MyReclamation";
    Layout = "~/Views/Shared/_MyTemplatepage.cshtml";
}

<h2>MyReclamation</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { id = ViewBag.us })
</p>

@{ if (TempData["erreur"] != null)
    {
    @section js{
        <script src="~/Scripts/jquery-2.2.4.min.js"></script>
        <script src="~/Scripts/toastr.min.js"></script>
        <link href="~/Content/toastr.min.css" rel="stylesheet" />

        <script type="text/javascript">

            $(document).ready(function () {

                toastr.error('Reclamation already treat');


            });
        </script>
    }}

}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.objet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>

        <th></th>
   
        </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.objet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>

            <td>

                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |

                @Html.ActionLink("Delete", "Delete", new { id = item.id })
               
                   
                
                </td>
        </tr>
    }

</table>
