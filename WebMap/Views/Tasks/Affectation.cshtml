@model IEnumerable<WebMap.Models.TasksModels>

@{
    Layout = "~/Views/Shared/_MyTemplatepage.cshtml";
}



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.nuser)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nmod)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)

            <td>
                @Html.DisplayFor(modelItem => item.nuser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nmod)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>
